    int contador = 0;
}

@section Styles {
    <link rel="stylesheet" href="~/css/main_page.css">
}


<div id="seccionAgregarAsesores">
    <h3>Agregar Asesores</h3>
    <div class="row">
        <div>
            <div class="alert alert-info" role="alert">
